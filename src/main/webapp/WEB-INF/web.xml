<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" 
	id="WebApp_ID" version="3.0">
	<!-- Filter ordering -->
	<filter-mapping>
	<filter-name>AuthFilter</filter-name>
		<url-pattern />
	</filter-mapping>
	<filter-mapping>
		<filter-name>RequestFilter</filter-name>
		<url-pattern />
	</filter-mapping>
	
	<!-- error pages -->
	<error-page>
		<error-code>400</error-code>
        <location>/jsp/views/400.jsp</location>
    </error-page>
    <error-page>
		<error-code>401</error-code>
        <location>/jsp/views/401.jsp</location>
    </error-page>
    <error-page>
		<error-code>403</error-code>
        <location>/jsp/views/403.jsp</location>
    </error-page>
	<error-page>
		<error-code>404</error-code>
        <location>/jsp/views/404.jsp</location>
    </error-page>
    <error-page>
		<exception-type>java.lang.Throwable</exception-type>
		<location>/jsp/views/500.jsp</location>
	</error-page>
	
	<!-- welcome files -->
	<welcome-file-list>
		<welcome-file>jsp/views/index.jsp</welcome-file>
	</welcome-file-list>
	
</web-app>